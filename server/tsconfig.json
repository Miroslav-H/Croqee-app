{
  "compilerOptions": {
    "sourceMap": true,
    "esModuleInterop": true,
    "allowJs": false,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["esNext"],
    "module": "commonjs",
    "target": "es2019",
    "outDir": "./dist"
  },
  "include": ["src/**/*"]
}
